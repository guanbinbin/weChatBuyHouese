/**app.wxss**/
@import "/css/font-awesome-size.wxss";
@import "/css/font-awesome.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.middle{
  margin: 0 auto;
}
.fa{
  font-size: auto;
}
pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
}
.title{
  font-size:14px;
}
.addr{
  font-size:12px;
  color:rgba(170, 170, 170, .5)
}
.price{
  color:red;
  font-size: 14px;
}
.room,.size{
  font-size:12px;
}
.subTitle{
  font-size: 12px;
}
.block{
  display: block;
}